{
  "name": "revvy",
  "version": "1.0.0",
  "description": "Revolt bot built with revolt.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && coffee --output dist --compile src",
    "dev": "tsc-watch --onSuccess \"$NODE --experimental-loader ./loader.js . dev\"",
    "start": "\"$NODE\" --experimental-loader ./loader.js dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexisother/revvy.git"
  },
  "keywords": [
    "revolt",
    "revolt.js",
    "bot"
  ],
  "author": "Alyxia Sother",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lexisother/revvy/issues"
  },
  "homepage": "https://github.com/lexisother/revvy#readme",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.23",
    "@types/ws": "^8.5.3",
    "coffeescript": "^2.6.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "inquirer": "^8.2.2",
    "node-fetch": "^3.2.3",
    "revolt-api": "^0.5.3-rc.8",
